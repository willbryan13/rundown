{
    "manifest_version": 3,
    "name": "Rundown",
    "version": "0.1",
    "permissions": [
        "tabs",
        "activeTab",
        "serviceWorker"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "optional_permissions": [
        "webNavigation"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "fetch_data.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "popup/popup.js"
            ],
            "matches": []
        },
        {
            "resources": [
                "background.js"
            ],
            "matches": []
        },
        {
            "resources": [
                "popup/popup.css"
            ],
            "matches": []
        },
        {
            "resources": [
                "fetch_data.js"
            ],
            "matches": []
        }
    ]
}