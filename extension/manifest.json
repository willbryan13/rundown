{
    "manifest_version": 3,
    "name": "Rundown",
    "version": "0.1",
    "permissions": [
        "tabs",
        "activeTab",
        "http://example.com/*",
        "serviceWorker"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "optional_permissions": [
        "webNavigation"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "popup/popup.js"
            ],
            "matches": []
        },
        {
            "resources": [
                "background.js"
            ],
            "matches": []
        },
        {
            "resources": [
                "popup/popup.css"
            ],
            "matches": []
        }
    ]
}